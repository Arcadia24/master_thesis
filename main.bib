@article{hussainAVRMicrocontrollerImplementation2011,
  title = {{{AVR Microcontroller Implementation}} for {{Customized Sound Generation}}},
  author = {Hussain, Tanvir and Haque, Md Mejbaul},
  year = {2011},
  month = nov,
  journal = {International Journal of Electrical and Computer Engineering (IJECE)},
  volume = {2},
  number = {1},
